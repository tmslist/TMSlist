---
import ContentPage from '../../layouts/ContentPage.astro';
import { PROVIDER_SLUGS } from '../../utils/seo_slugs';

export function getStaticPaths() {
  return PROVIDER_SLUGS.map(({ slug, title }) => ({
    params: { slug },
    props: { title },
  }));
}

const { title } = Astro.props;
---

<ContentPage title={title} subtitle="Find verified providers and clinics matching your specific needs." />
