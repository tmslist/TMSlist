---
import ContentPage from '../../../layouts/ContentPage.astro';
import { COMMERCIAL_SLUGS } from '../../../utils/seo_slugs';

export function getStaticPaths() {
  return COMMERCIAL_SLUGS.map(({ slug, title }) => ({
    params: { slug },
    props: { title },
  }));
}

const { title } = Astro.props;
---

<ContentPage title={title} subtitle="Business solutions and growth strategies for TMS clinics." />
