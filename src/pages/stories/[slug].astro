---
import ContentPage from '../../layouts/ContentPage.astro';
import { STORIES_SLUGS } from '../../utils/seo_slugs';

export function getStaticPaths() {
  return STORIES_SLUGS.map(({ slug, title }) => ({
    params: { slug },
    props: { title },
  }));
}

const { title } = Astro.props;
---

<ContentPage title={title} subtitle="Real stories from real patients." />
