---
import ContentPage from '../../layouts/ContentPage.astro';
import { BLOG_SLUGS } from '../../utils/seo_slugs';

export function getStaticPaths() {
  return BLOG_SLUGS.map(({ slug, title }) => ({
    params: { slug },
    props: { title },
  }));
}

const { title } = Astro.props;
---

<ContentPage title={title} subtitle="Expert insights and latest updates on TMS therapy." />
