---
import ContentPage from '../../layouts/ContentPage.astro';
import { TREATMENT_SLUGS } from '../../utils/seo_slugs';

export function getStaticPaths() {
  return TREATMENT_SLUGS.map(({ slug, title }) => ({
    params: { slug },
    props: { title },
  }));
}

const { title } = Astro.props;
---

<ContentPage title={title} subtitle="Evidence-based information on treating this condition with TMS." />
