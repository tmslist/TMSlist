---
import ContentPage from '../../layouts/ContentPage.astro';
import { DEMOGRAPHIC_SLUGS } from '../../utils/seo_slugs';

export function getStaticPaths() {
  return DEMOGRAPHIC_SLUGS.map(({ slug, title }) => ({
    params: { slug },
    props: { title },
  }));
}

const { title } = Astro.props;
---

<ContentPage title={title} subtitle="Specialized TMS considerations for specific patient groups." />
